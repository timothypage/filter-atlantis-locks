{
  "name": "Atlantis Filter Extension",
  "description": "Extension to filter Atlantis locks page",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["https://atlantis-ipn.ops.int.ibops.net/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}